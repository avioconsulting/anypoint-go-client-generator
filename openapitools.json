{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.1.0",
    "generators": {
      "golang": {
        "generatorName": "go",
        "output": "#{name}",
        "glob": "spec/apm.yml",
        "gitUserId": "avioconsulting",
        "gitRepoId": "anypoint-go-client-generator",
        "additionalProperties": {
          "isGoSubmodule": false,
          "packageName": "#{name}",
          "structPrefix": true,
          "packageVersion": "1.0.0"
        }
      }
    }
  }
}
